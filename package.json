{
  "name": "paas-admin",
  "private": true,
  "version": "0.1.0",
  "description": "GOV.UK PaaS CloudFoundry UI",
  "main": "src/main.ts",
  "repository": "https://github.com/alphagov/paas-admin",
  "scripts": {
    "build": "npm run -s clean && webpack --config config/webpack.config.js",
    "watch": "ENABLE_WATCH=true ENABLE_TESTS=true npm run -s build",
    "test": "npm run -s build && ENABLE_COVERAGE=true npm run -s test:unit && npm run -s lint",
    "test:unit": "jest --coverage",
    "test:audit": "npm audit",
    "test:vulnerabilities": "if npm audit | grep -E -B9999 '\\d+\\s+(high|critical)'; then npm audit; else exit 0; fi",
    "test:acceptance": "jest ./acceptance-tests/main.test.ts --config=acceptance-tests/jest-config.json --silent=false",
    "lint": "npm run -s lint:ts && npm run -s lint:sass",
    "lint:sass": "stylelint '**/*.scss'",
    "lint:ts": "eslint --quiet --format stylish --ext .js,.jsx,.ts,.tsx .",
    "fix": "npm run -s fix:ts",
    "fix:sass": "stylelint '**/*.scss' --fix",
    "fix:ts": "eslint --fix --ext .js,.jsx,.ts,.tsx .",
    "fix:vulnerabilities": "npm audit fix --update-binary",
    "start": "export PORT=${PORT-3000}; export LOG_LEVEL=${LOG_LEVEL-warn}; if [ \"$NODE_ENV\" = 'production' ]; then npm run -s start:production; else npm run -s start:dev; fi",
    "start:dev": "NODE_ENV=development ENABLE_WATCH=true ENABLE_SERVER=true ALLOW_INSECURE_SESSION=true npm run -s build",
    "start:dev:with-cf-dev-env": "./config/use-dev-env.sh",
    "start:stub-api": "./stub-api/start-stub-api.sh",
    "start:with-stub-api": "./stub-api/start-with-stub-api.sh",
    "start:production": "NODE_ENV=production ALLOW_INSECURE_SESSION=true npm run -s build && node dist/main.js",
    "push": "NODE_ENV=production npm run -s build && cf push",
    "clean": "find dist -maxdepth 1 ! -name downloads ! -name dist -exec rm -r {} \\; || echo 'already clean'"
  },
  "author": "GOV.UK PaaS Team",
  "license": "MIT",
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@types/aws-sdk": "^2.7.0",
    "@types/body-parser": "^1.19.2",
    "@types/cheerio": "^0.22.31",
    "@types/compression": "^1.7.2",
    "@types/cookie-session": "^2.0.44",
    "@types/csurf": "^1.11.2",
    "@types/d3-array": "^3.0.1",
    "@types/d3-axis": "^3.0.1",
    "@types/d3-scale": "^4.0.1",
    "@types/d3-selection": "^3.0.1",
    "@types/d3-shape": "^3.0.2",
    "@types/eslint": "^8.4.10",
    "@types/express": "^4.17.15",
    "@types/jest": "^29.2.5",
    "@types/jsdom": "^20.0.1",
    "@types/jsonwebtoken": "^9.0.0",
    "@types/lodash": "^4.14.191",
    "@types/node-jose": "^1.1.10",
    "@types/passport": "^1.0.7",
    "@types/passport-oauth2": "^1.4.11",
    "@types/pino": "^6.3.12",
    "@types/qs": "^6.9.7",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.10",
    "@types/route-parser": "^0.1.4",
    "@types/source-map-support": "^0.5.6",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.48.1",
    "@typescript-eslint/parser": "^5.48.1",
    "body-parser": "^1.20.1",
    "cheerio": "^1.0.0-rc.12",
    "compression-webpack-plugin": "^10.0.0",
    "css-loader": "^6.7.3",
    "css-minimizer-webpack-plugin": "^4.2.2",
    "csv-parse": "^5.3.3",
    "eslint": "^8.31.0",
    "eslint-config-react": "^1.1.7",
    "eslint-plugin-functional": "^4.4.1",
    "eslint-plugin-import": "^2.27.4",
    "eslint-plugin-jsx-a11y": "^6.7.0",
    "eslint-plugin-react": "^7.32.0",
    "expect": "^29.3.0",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "jest-environment-node": "^29.2.0",
    "jest-puppeteer": "^6.2.0",
    "lodash": "^4.17.21",
    "mini-css-extract-plugin": "^2.7.2",
    "nock": "^13.3.0",
    "nodemon-webpack-plugin": "^4.8.1",
    "pino-pretty": "^7.6.1",
    "puppeteer": "^19.5.1",
    "sass": "^1.57.1",
    "sass-loader": "^13.2.0",
    "stylelint": "^14.16.1",
    "stylelint-config-gds": "^0.2.0",
    "stylelint-config-standard-scss": "^6.1.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.0.3",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-node-externals": "^3.0.0"
  },
  "dependencies": {
    "@aws-sdk/client-cloudwatch": "^3.245.0",
    "@aws-sdk/client-resource-groups-tagging-api": "^3.245.0",
    "@aws-sdk/types": "^3.226.0",
    "@babel/preset-env": "^7.19.4",
    "@babel/preset-react": "^7.18.6",
    "aws-sdk": "^2.1294.0",
    "axios": "^0.27.2",
    "babel-jest": "^29.2.0",
    "base32-encode": "^1.2.0",
    "compression": "^1.7.4",
    "cookie-session": "^2.0.0",
    "csurf": "^1.11.0",
    "d3": "^7.8.1",
    "d3-array": "^2.12.1",
    "d3-axis": "^2.1.0",
    "d3-sankey": "^0.12.3",
    "d3-scale": "^3.3.0",
    "d3-selection": "^2.0.0",
    "d3-shape": "^2.1.0",
    "date-fns": "^2.29.3",
    "express": "^4.18.2",
    "express-pino-logger": "^7.0.0",
    "express-static-gzip": "^2.1.7",
    "fnv-plus": "^1.3.1",
    "govuk-frontend": "^4.4.1",
    "happy-dom": "^8.1.3",
    "helmet": "^6.0.1",
    "jsonwebtoken": "^9.0.0",
    "lodash": "^4.17.21",
    "nanoid": "^3.3.4",
    "node-jose": "^2.1.1",
    "node-zendesk": "^2.2.0",
    "notifications-node-client": "^6.0.0",
    "openid-client": "^4.9.1",
    "p-limit": "^3.1.0",
    "passport": "^0.5.2",
    "passport-oauth2": "^1.6.1",
    "pino": "^6.13.3",
    "qs": "^6.11.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-markdown": "^6.0.3",
    "route-parser": "^0.0.5",
    "source-map-support": "^0.5.21",
    "tslib": "^2.4.1"
  },
  "overrides": {
    "request@2.88.2": {
      "qs": "^6.11.0"
    }
  },
  "engines": {
    "node": "16.x.x",
    "npm": ">=8.x.x"
  },
  "jest": {
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "<rootDir>/config/",
      "<rootDir>/src/components/errors/",
      "index.ts"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ],
    "moduleNameMapper": {
      "^.+\\.(css|scss|sass)$": "<rootDir>/config/jest/file.js"
    },
    "transform": {
      "^.+\\.(js|jsx)$": "babel-jest",
      "^.+\\.(ts|tsx)$": [
        "ts-jest",
        {
          "tsconfig": "tsconfig.json",
          "diagnostics": false
        }
      ],
      "^.+\\.(css|scss|sass)$": "<rootDir>/config/jest/file.js",
      "^.+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/config/jest/file.js"
    },
    "testMatch": [
      "<rootDir>/src/**/*.test.+(ts|tsx|js|jsx)"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/config/jest/jest-setup.js"
    ]
  }
}

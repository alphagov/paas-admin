{
  "name": "paas-admin",
  "private": true,
  "version": "0.1.0",
  "description": "GOV.UK PaaS CloudFoundry UI",
  "main": "src/main.ts",
  "repository": "https://github.com/alphagov/paas-admin",
  "scripts": {
    "build": "npm run -s clean && webpack --config config/webpack.config.js",
    "watch": "ENABLE_WATCH=true ENABLE_TESTS=true npm run -s build",
    "test": "npm run -s build && ENABLE_COVERAGE=true npm run -s test:unit && npm run -s lint",
    "test:unit": "jest --coverage",
    "test:audit": "npm audit",
    "test:vulnerabilities": "if npm audit | grep -E -B9999 '\\d+\\s+(high|critical)'; then npm audit; else exit 0; fi",
    "test:acceptance": "jest ./acceptance-tests/main.test.ts --config=acceptance-tests/jest-config.json --silent=false",
    "lint": "npm run -s lint:ts && npm run -s lint:sass",
    "lint:sass": "stylelint '**/*.scss'",
    "lint:ts": "eslint --quiet --format stylish --ext .js,.jsx,.ts,.tsx .",
    "fix": "npm run -s fix:ts",
    "fix:sass": "stylelint '**/*.scss' --fix",
    "fix:ts": "eslint --fix --ext .js,.jsx,.ts,.tsx .",
    "fix:vulnerabilities": "npm audit fix --update-binary",
    "start": "export PORT=${PORT-3000}; export LOG_LEVEL=${LOG_LEVEL-warn}; if [ \"$NODE_ENV\" = 'production' ]; then npm run -s start:production; else npm run -s start:dev; fi",
    "start:dev": "NODE_ENV=development ENABLE_WATCH=true ENABLE_SERVER=true ALLOW_INSECURE_SESSION=true npm run -s build",
    "start:dev:with-cf-dev-env": "./config/use-dev-env.sh",
    "start:stub-api": "./stub-api/start-stub-api.sh",
    "start:with-stub-api": "./stub-api/start-with-stub-api.sh",
    "start:production": "NODE_ENV=production ALLOW_INSECURE_SESSION=true npm run -s build && node dist/main.js",
    "push": "NODE_ENV=production npm run -s build && cf push",
    "clean": "find dist -maxdepth 1 ! -name downloads ! -name dist -exec rm -r {} \\; || echo 'already clean'"
  },
  "author": "GOV.UK PaaS Team",
  "license": "MIT",
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.3",
    "@testing-library/react": "^14.0.0",
    "@types/aws-sdk": "^2.7.0",
    "@types/body-parser": "^1.19.3",
    "@types/cheerio": "^0.22.32",
    "@types/compression": "^1.7.3",
    "@types/cookie-session": "^2.0.45",
    "@types/csurf": "^1.11.3",
    "@types/d3-array": "^3.0.1",
    "@types/d3-axis": "^3.0.1",
    "@types/d3-scale": "^4.0.1",
    "@types/d3-selection": "^3.0.1",
    "@types/d3-shape": "^3.0.2",
    "@types/eslint": "^8.44.3",
    "@types/express": "^4.17.18",
    "@types/jest": "^29.5.5",
    "@types/jsdom": "^21.1.3",
    "@types/jsonwebtoken": "^9.0.3",
    "@types/lodash": "^4.14.198",
    "@types/node-jose": "^1.1.11",
    "@types/passport": "^1.0.7",
    "@types/passport-oauth2": "^1.4.13",
    "@types/pino": "^6.3.12",
    "@types/qs": "^6.9.8",
    "@types/react": "^18.2.24",
    "@types/react-dom": "^18.2.8",
    "@types/route-parser": "^0.1.5",
    "@types/source-map-support": "^0.5.8",
    "@types/supertest": "^2.0.13",
    "@typescript-eslint/eslint-plugin": "^5.61.0",
    "@typescript-eslint/parser": "^5.61.0",
    "body-parser": "^1.20.2",
    "cheerio": "^1.0.0-rc.12",
    "compression-webpack-plugin": "^10.0.0",
    "css-loader": "^6.8.1",
    "css-minimizer-webpack-plugin": "^5.0.1",
    "csv-parse": "^5.5.0",
    "eslint": "^8.50.0",
    "eslint-config-react": "^1.1.7",
    "eslint-plugin-functional": "^5.0.8",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.33.2",
    "expect": "^29.7.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-environment-node": "^29.7.0",
    "jest-puppeteer": "^9.0.0",
    "lodash": "^4.17.21",
    "mini-css-extract-plugin": "^2.7.6",
    "nock": "^13.3.3",
    "nodemon-webpack-plugin": "^4.8.1",
    "pino-pretty": "^7.6.1",
    "puppeteer": "^21.3.6",
    "sass": "^1.68.0",
    "sass-loader": "^13.3.2",
    "stylelint": "^14.16.1",
    "stylelint-config-gds": "^0.3.0",
    "stylelint-config-standard-scss": "^6.1.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.4.4",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4",
    "webpack-node-externals": "^3.0.0"
  },
  "dependencies": {
    "@aws-sdk/client-cloudwatch": "^3.421.0",
    "@aws-sdk/client-resource-groups-tagging-api": "^3.421.0",
    "@aws-sdk/types": "^3.418.0",
    "@aws-sdk/client-rds": "^3.421.0",
    "@babel/preset-env": "^7.22.20",
    "@babel/preset-react": "^7.22.15",
    "axios": "^0.27.2",
    "babel-jest": "^29.7.0",
    "base32-encode": "^1.2.0",
    "compression": "^1.7.4",
    "cookie-session": "^2.0.0",
    "csurf": "^1.11.0",
    "d3": "^7.8.5",
    "d3-array": "^2.12.1",
    "d3-axis": "^2.1.0",
    "d3-sankey": "^0.12.3",
    "d3-scale": "^3.3.0",
    "d3-selection": "^2.0.0",
    "d3-shape": "^2.1.0",
    "date-fns": "^2.30.0",
    "express": "^4.18.2",
    "express-pino-logger": "^7.0.0",
    "express-static-gzip": "^2.1.7",
    "fnv-plus": "^1.3.1",
    "govuk-frontend": "^4.7.0",
    "happy-dom": "^12.2.1",
    "helmet": "^7.0.0",
    "html-entities": "^2.4.0",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "nanoid": "^3.3.6",
    "node-jose": "^2.2.0",
    "node-zendesk": "^3.0.3",
    "notifications-node-client": "^7.0.3",
    "openid-client": "^4.9.1",
    "p-limit": "^3.1.0",
    "passport": "^0.5.2",
    "passport-oauth2": "^1.7.0",
    "pino": "^6.13.3",
    "qs": "^6.11.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-markdown": "^6.0.3",
    "route-parser": "^0.0.5",
    "source-map-support": "^0.5.21",
    "tslib": "^2.6.2"
  },
  "overrides": {
    "request@2.88.2": {
      "qs": "^6.11.0",
      "tough-cookie": "^4.1.3"
    }
  },
  "engines": {
    "node": "18.x.x",
    "npm": ">=8.x.x"
  },
  "jest": {
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "<rootDir>/config/",
      "<rootDir>/src/components/errors/",
      "index.ts"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ],
    "moduleNameMapper": {
      "^.+\\.(css|scss|sass)$": "<rootDir>/config/jest/file.js"
    },
    "transform": {
      "^.+\\.(js|jsx)$": "babel-jest",
      "^.+\\.(ts|tsx)$": [
        "ts-jest",
        {
          "tsconfig": "tsconfig.json",
          "diagnostics": false
        }
      ],
      "^.+\\.(css|scss|sass)$": "<rootDir>/config/jest/file.js",
      "^.+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/config/jest/file.js"
    },
    "testMatch": [
      "<rootDir>/src/**/*.test.+(ts|tsx|js|jsx)"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/config/jest/jest-setup.js"
    ]
  }
}

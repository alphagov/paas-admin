{
  "name": "paas-admin",
  "private": true,
  "version": "0.1.0",
  "description": "GOV.UK PaaS CloudFoundry UI",
  "main": "src/main.ts",
  "repository": "https://github.com/alphagov/paas-admin",
  "scripts": {
    "build": "npm run -s clean && webpack --config config/webpack.config.js",
    "watch": "ENABLE_WATCH=true ENABLE_TESTS=true npm run -s build",
    "test": "npm run -s build && ENABLE_COVERAGE=true npm run -s test:unit && npm run -s lint",
    "test:unit": "jest --coverage",
    "test:audit": "npm audit",
    "test:vulnerabilities": "if npm audit | grep -E -B9999 '\\d+\\s+(high|critical)'; then npm audit; else exit 0; fi",
    "test:acceptance": "ts-mocha --exit acceptance-tests/**/*.test.ts",
    "lint": "npm run -s lint:ts && npm run -s lint:sass",
    "lint:sass": "sass-lint -v",
    "lint:ts": "eslint --quiet --format codeframe --ext .js,.jsx,.ts,.tsx .",
    "fix": "npm run -s fix:ts",
    "fix:sass": "sass-lint --fix ./src",
    "fix:ts": "eslint --fix --ext .js,.jsx,.ts,.tsx .",
    "fix:vulnerabilities": "npm audit fix --update-binary",
    "start": "export PORT=${PORT-3000}; export LOG_LEVEL=${LOG_LEVEL-warn}; if [ \"$NODE_ENV\" = 'production' ]; then npm run -s start:production; else npm run -s start:dev; fi",
    "start:dev": "NODE_ENV=development ENABLE_WATCH=true ENABLE_SERVER=true ALLOW_INSECURE_SESSION=true npm run -s build",
    "start:stub-api": "source ./stub-api/stub-apis-env.sh; ts-node stub-api/index.ts",
    "start:with-stub-api": "source ./stub-api/stub-apis-env.sh; npm start",
    "start:production": "NODE_ENV=production ALLOW_INSECURE_SESSION=true npm run -s build && node dist/main.js",
    "push": "NODE_ENV=production npm run -s build && cf push",
    "clean": "rm -rf dist/* || echo 'already clean'"
  },
  "author": "GOV.UK PaaS Team",
  "license": "MIT",
  "devDependencies": {
    "@intervolga/optimize-cssnano-plugin": "^1.0.6",
    "@types/body-parser": "^1.19.0",
    "@types/cheerio": "^0.22.17",
    "@types/compression": "^1.7.0",
    "@types/cookie-session": "^2.0.39",
    "@types/csurf": "^1.9.36",
    "@types/d3-array": "^2.0.0",
    "@types/d3-axis": "^1.0.12",
    "@types/d3-scale": "^2.2.0",
    "@types/d3-selection": "^1.4.1",
    "@types/d3-shape": "^1.3.2",
    "@types/enzyme": "^3.10.5",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/eslint": "^6.8.0",
    "@types/express": "^4.17.4",
    "@types/helmet": "^0.0.45",
    "@types/jest": "^25.2.1",
    "@types/jsdom": "^16.2.0",
    "@types/jsonwebtoken": "^8.3.9",
    "@types/lodash": "^4.14.149",
    "@types/moment": "^2.13.0",
    "@types/node-jose": "^1.1.2",
    "@types/passport": "^1.0.3",
    "@types/passport-oauth2": "^1.4.9",
    "@types/pino": "^5.17.0",
    "@types/qs": "^6.9.1",
    "@types/react": "^16.9.33",
    "@types/react-dom": "^16.9.6",
    "@types/route-parser": "^0.1.3",
    "@types/source-map-support": "^0.5.1",
    "@types/supertest": "^2.0.8",
    "@types/uuid": "^7.0.2",
    "@typescript-eslint/eslint-plugin": "^2.27.0",
    "@typescript-eslint/parser": "^2.27.0",
    "body-parser": "^1.19.0",
    "cheerio": "^1.0.0-rc.3",
    "compression-webpack-plugin": "^3.1.0",
    "css-loader": "^3.5.1",
    "csv-parse": "^4.8.8",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^6.8.0",
    "eslint-config-react": "^1.1.7",
    "eslint-plugin-functional": "^3.0.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.19.0",
    "expect": "^25.2.7",
    "extract-loader": "^5.0.1",
    "file-loader": "^6.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^25.2.7",
    "loader-utils": "^2.0.0",
    "lodash": "^4.17.15",
    "mocha": "^7.1.1",
    "nock": "^12.0.3",
    "node-sass": "^4.13.1",
    "nodemon-webpack-plugin": "^4.3.1",
    "pegjs": "^0.10.0",
    "pegjs-loader": "^0.5.6",
    "sass-lint": "^1.13.1",
    "sass-loader": "^8.0.2",
    "supertest": "^4.0.2",
    "ts-jest": "^25.3.1",
    "ts-loader": "^6.2.2",
    "ts-mocha": "^7.0.0",
    "ts-node": "^8.8.2",
    "typescript": "^3.8.3",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11",
    "webpack-node-externals": "^1.7.2",
    "zombie": "^6.1.4"
  },
  "dependencies": {
    "@aws-sdk/client-cloudwatch-node": "^0.1.0-preview.2",
    "@aws-sdk/client-resource-groups-tagging-api-node": "0.1.0-preview.2",
    "axios": "^0.19.2",
    "base32-encode": "^1.1.1",
    "compression": "^1.7.4",
    "cookie-session": "^2.0.0-rc.1",
    "csurf": "^1.11.0",
    "d3": "^5.15.1",
    "d3-array": "^2.4.0",
    "d3-axis": "^1.0.12",
    "d3-sankey": "^0.12.3",
    "d3-scale": "^3.2.1",
    "d3-selection": "^1.4.1",
    "d3-shape": "^1.3.7",
    "express": "^4.17.1",
    "express-pino-logger": "^4.0.0",
    "express-static-gzip": "^2.0.6",
    "fnv-plus": "^1.3.1",
    "govuk-frontend": "^3.6.0",
    "helmet": "^3.22.0",
    "jsdom": "^16.2.2",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "nanoid": "^3.0.2",
    "node-jose": "^1.1.4",
    "notifications-node-client": "^4.7.2",
    "openid-client": "^3.14.2",
    "p-limit": "^2.3.0",
    "passport": "^0.4.1",
    "passport-oauth2": "^1.5.0",
    "pino": "^6.0.0",
    "qs": "^6.9.3",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-markdown": "^4.3.1",
    "route-parser": "^0.0.5",
    "source-map-support": "^0.5.16",
    "tslib": "^1.11.1",
    "uuid": "^7.0.3"
  },
  "engines": {
    "node": "12.x.x",
    "npm": ">=6.x.x"
  },
  "jest": {
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "<rootDir>/config/",
      "<rootDir>/src/components/errors/",
      "index.ts"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ],
    "moduleNameMapper": {
      "^.+\\.(css|scss|sass)$": "<rootDir>/config/jest/file.js"
    },
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest",
      "^.+\\.(css|scss|sass)$": "identity-obj-proxy",
      "^.+\\.(pegjs)$": "<rootDir>/config/jest/pegjs.js",
      "^.+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/config/jest/file.js"
    },
    "globals": {
      "ts-jest": {
        "tsConfig": "tsconfig.json",
        "diagnostics": false
      }
    },
    "testMatch": [
      "<rootDir>/src/**/*.test.+(ts|tsx|js|jsx)"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/config/jest/enzyme.config.js"
    ]
  }
}

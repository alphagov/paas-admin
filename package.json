{
  "name": "paas-admin",
  "private": true,
  "version": "0.1.0",
  "description": "GOV.UK PaaS CloudFoundry UI",
  "main": "src/main.ts",
  "repository": "https://github.com/alphagov/paas-admin",
  "scripts": {
    "build": "npm run -s clean && webpack --config config/webpack.config.js",
    "watch": "ENABLE_WATCH=true ENABLE_TESTS=true npm run -s build",
    "test": "npm run -s build && ENABLE_COVERAGE=true npm run -s test:unit && npm run -s lint",
    "test:unit": "jest --coverage",
    "test:audit": "npm audit",
    "test:vulnerabilities": "if npm audit | grep -E -B9999 '\\d+\\s+(high|critical)'; then npm audit; else exit 0; fi",
    "test:acceptance": "ts-mocha --exit acceptance-tests/**/*.test.ts",
    "lint": "npm run -s lint:ts && npm run -s lint:sass",
    "lint:ts": "tslint --format codeFrame -p .",
    "lint:sass": "sass-lint -v",
    "fix": "npm run -s fix:js",
    "fix:ts": "tslint -p . --fix",
    "fix:sass": "sass-lint --fix ./src",
    "fix:vulnerabilities": "npm audit fix --update-binary",
    "start": "export PORT=${PORT-3000}; export LOG_LEVEL=${LOG_LEVEL-warn}; if [ \"$NODE_ENV\" = 'production' ]; then npm run -s start:production; else npm run -s start:dev; fi",
    "start:dev": "NODE_ENV=development ENABLE_WATCH=true ENABLE_SERVER=true ALLOW_INSECURE_SESSION=true npm run -s build",
    "start:stub-api": "source ./stub-api/stub-apis-env.sh; ts-node stub-api/index.ts",
    "start:with-stub-api": "source ./stub-api/stub-apis-env.sh; npm start",
    "start:production": "NODE_ENV=production ALLOW_INSECURE_SESSION=true npm run -s build && node dist/main.js",
    "push": "NODE_ENV=production npm run -s build && cf push",
    "clean": "rm -rf dist/* || echo 'already clean'"
  },
  "author": "GOV.UK PaaS Team",
  "license": "MIT",
  "devDependencies": {
    "@intervolga/optimize-cssnano-plugin": "^1.0.6",
    "@types/cheerio": "^0.22.15",
    "@types/compression": "^1.0.1",
    "@types/csurf": "^1.9.36",
    "@types/d3-array": "^2.0.0",
    "@types/d3-axis": "^1.0.12",
    "@types/d3-format": "^1.3.1",
    "@types/d3-scale": "^2.1.1",
    "@types/d3-selection": "^1.4.1",
    "@types/d3-shape": "^1.3.2",
    "@types/d3-time-format": "^2.1.1",
    "@types/express": "^4.17.2",
    "@types/helmet": "^0.0.45",
    "@types/jest": "^24.0.23",
    "@types/jsdom": "^12.2.4",
    "@types/jsonwebtoken": "^8.3.5",
    "@types/lodash": "^4.14.149",
    "@types/moment": "^2.13.0",
    "@types/nanoid": "^2.1.0",
    "@types/node-jose": "^1.1.1",
    "@types/passport": "^1.0.2",
    "@types/passport-oauth2": "^1.4.8",
    "@types/pino": "^5.14.0",
    "@types/qs": "^6.9.0",
    "@types/route-parser": "^0.1.3",
    "@types/source-map-support": "^0.5.0",
    "@types/supertest": "^2.0.8",
    "@types/uuid": "^3.4.6",
    "@types/webpack-env": "^1.14.1",
    "cheerio": "^1.0.0-rc.3",
    "compression-webpack-plugin": "^3.0.0",
    "css-loader": "^3.2.1",
    "csv-parse": "^4.8.2",
    "expect": "^24.9.0",
    "extract-loader": "^3.1.0",
    "file-loader": "^5.0.2",
    "html-loader": "^0.5.5",
    "identity-obj-proxy": "^3.0.0",
    "imports-loader": "^0.8.0",
    "jest": "^24.9.0",
    "loader-utils": "^1.2.3",
    "lodash": "^4.17.15",
    "mocha": "^6.2.2",
    "nock": "^11.7.0",
    "node-sass": "^4.13.0",
    "nodemon-webpack-plugin": "^4.2.1",
    "pegjs": "^0.10.0",
    "pegjs-loader": "^0.5.5",
    "sass-lint": "^1.13.1",
    "sass-loader": "^8.0.0",
    "schema-utils": "^2.6.1",
    "supertest": "^4.0.2",
    "ts-jest": "^24.2.0",
    "ts-loader": "^6.2.1",
    "ts-mocha": "^6.0.0",
    "ts-node": "^8.5.4",
    "tslint": "^5.20.1",
    "tslint-consistent-codestyle": "^1.16.0",
    "tslint-immutable": "^6.0.1",
    "tslint-microsoft-contrib": "^6.2.0",
    "typescript": "^3.7.3",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-node-externals": "^1.7.2",
    "zombie": "^6.1.4"
  },
  "dependencies": {
    "@aws-sdk/client-cloudwatch-node": "^0.1.0-preview.2",
    "axios": "^0.19.0",
    "base32-encode": "^1.1.1",
    "compression": "^1.7.4",
    "cookie-session": "^2.0.0-beta.3",
    "csurf": "^1.10.0",
    "d3": "^5.14.2",
    "d3-array": "^2.4.0",
    "d3-axis": "^1.0.12",
    "d3-format": "^1.4.2",
    "d3-sankey": "^0.12.3",
    "d3-scale": "^3.2.1",
    "d3-selection": "^1.4.1",
    "d3-shape": "^1.3.7",
    "d3-time-format": "^2.2.2",
    "express": "^4.17.1",
    "express-pino-logger": "^4.0.0",
    "express-static-gzip": "^2.0.5",
    "fnv-plus": "^1.3.1",
    "govuk-frontend": "^3.4.0",
    "helmet": "^3.21.2",
    "jmespath": "^0.15.0",
    "jsdom": "^15.2.1",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.15",
    "merge-deep": "^3.0.2",
    "moment": "^2.24.0",
    "nanoid": "^2.1.7",
    "node-jose": "^1.1.3",
    "notifications-node-client": "^4.7.0",
    "nunjucks": "^3.2.0",
    "openid-client": "^3.10.0",
    "p-limit": "^2.2.1",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "passport-oauth2": "^1.5.0",
    "pino": "^5.14.0",
    "qs": "^6.9.1",
    "route-parser": "^0.0.5",
    "showdown": "^1.9.1",
    "source-map-support": "^0.5.16",
    "tslib": "^1.10.0",
    "uuid": "^3.3.3"
  },
  "engines": {
    "node": "12.x.x",
    "npm": ">=6.x.x"
  },
  "jest": {
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/.+.njk",
      "<rootDir>/config/",
      "index.ts"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest",
      "^.+\\.(css|scss|sass)$": "identity-obj-proxy",
      "^.+\\.(html|njk)$": "<rootDir>/config/jest/njk.js",
      "^.+\\.(pegjs)$": "<rootDir>/config/jest/pegjs.js",
      "^.+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/config/jest/file.js"
    },
    "globals": {
      "ts-jest": {
        "tsConfig": "tsconfig.json",
        "diagnostics": false
      }
    },
    "testMatch": [
      "<rootDir>/src/**/*.test.+(ts|tsx|js|jsx)"
    ]
  }
}

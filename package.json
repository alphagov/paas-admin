{
  "name": "paas-admin",
  "private": true,
  "version": "0.1.0",
  "description": "GOV.UK PaaS CloudFoundry UI",
  "main": "src/main.ts",
  "repository": "https://github.com/alphagov/paas-admin",
  "scripts": {
    "build": "npm run -s clean && webpack --config config/webpack.config.js",
    "watch": "ENABLE_WATCH=true ENABLE_TESTS=true npm run -s build",
    "test": "npm run -s build && ENABLE_COVERAGE=true npm run -s test:unit && npm run -s lint",
    "test:unit": "jest --coverage",
    "test:audit": "npm audit",
    "test:vulnerabilities": "if npm audit | grep -E -B9999 '\\d+\\s+(high|critical)'; then npm audit; else exit 0; fi",
    "test:acceptance": "ts-mocha --exit acceptance-tests/**/*.test.ts",
    "lint": "npm run -s lint:ts && npm run -s lint:sass",
    "lint:sass": "stylelint '**/*.scss'",
    "lint:ts": "eslint --quiet --format codeframe --ext .js,.jsx,.ts,.tsx .",
    "fix": "npm run -s fix:ts",
    "fix:sass": "stylelint '**/*.scss' --fix",
    "fix:ts": "eslint --fix --ext .js,.jsx,.ts,.tsx .",
    "fix:vulnerabilities": "npm audit fix --update-binary",
    "scrape:performance": "ts-node src/scripts/performance.ts",
    "start": "export PORT=${PORT-3000}; export LOG_LEVEL=${LOG_LEVEL-warn}; if [ \"$NODE_ENV\" = 'production' ]; then npm run -s start:production; else npm run -s start:dev; fi",
    "start:dev": "NODE_ENV=development ENABLE_WATCH=true ENABLE_SERVER=true ALLOW_INSECURE_SESSION=true npm run -s build",
    "start:stub-api": "source ./stub-api/stub-apis-env.sh; ts-node stub-api/index.ts",
    "start:with-stub-api": "source ./stub-api/stub-apis-env.sh; npm start",
    "start:production": "NODE_ENV=production ALLOW_INSECURE_SESSION=true npm run -s build && node dist/main.js",
    "push": "NODE_ENV=production npm run -s build && cf push",
    "clean": "find dist -maxdepth 1 ! -name downloads ! -name dist -exec rm -r {} \\; || echo 'already clean'"
  },
  "author": "GOV.UK PaaS Team",
  "license": "MIT",
  "devDependencies": {
    "@intervolga/optimize-cssnano-plugin": "^1.0.6",
    "@types/aws-sdk": "^2.7.0",
    "@types/body-parser": "^1.19.0",
    "@types/cheerio": "^0.22.28",
    "@types/compression": "^1.7.0",
    "@types/cookie-session": "^2.0.42",
    "@types/csurf": "^1.11.0",
    "@types/d3-array": "^2.9.0",
    "@types/d3-axis": "^2.0.0",
    "@types/d3-scale": "^3.2.2",
    "@types/d3-selection": "^2.0.0",
    "@types/d3-shape": "^2.0.0",
    "@types/enzyme": "^3.10.8",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/eslint": "^7.2.8",
    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.22",
    "@types/jsdom": "^16.2.10",
    "@types/jsonwebtoken": "^8.5.1",
    "@types/lodash": "^4.14.168",
    "@types/moment": "^2.13.0",
    "@types/node-jose": "^1.1.5",
    "@types/passport": "^1.0.6",
    "@types/passport-oauth2": "^1.4.10",
    "@types/pino": "^6.3.6",
    "@types/qs": "^6.9.6",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.3",
    "@types/route-parser": "^0.1.3",
    "@types/source-map-support": "^0.5.3",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.21.0",
    "@typescript-eslint/parser": "^4.20.0",
    "body-parser": "^1.19.0",
    "cheerio": "^1.0.0-rc.5",
    "compression-webpack-plugin": "^6.1.1",
    "css-loader": "^4.2.1",
    "csv-parse": "^4.15.3",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "eslint": "^7.23.0",
    "eslint-config-react": "^1.1.7",
    "eslint-plugin-functional": "^3.2.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.23.1",
    "expect": "^26.6.2",
    "extract-loader": "^5.1.0",
    "file-loader": "^6.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "loader-utils": "^2.0.0",
    "lodash": "^4.17.21",
    "mocha": "^8.3.2",
    "nock": "^13.0.11",
    "nodemon-webpack-plugin": "^4.5.2",
    "pegjs": "^0.10.0",
    "pegjs-loader": "^0.5.6",
    "pino-pretty": "^4.7.1",
    "sass": "^1.32.8",
    "sass-loader": "^9.0.3",
    "stylelint": "^13.11.0",
    "stylelint-config-gds": "^0.1.0",
    "supertest": "^6.1.3",
    "ts-jest": "^26.5.4",
    "ts-loader": "^8.1.0",
    "ts-mocha": "^8.0.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.3",
    "webpack": "^4.46.0",
    "webpack-cli": "^4.6.0",
    "webpack-node-externals": "^2.5.2",
    "zombie": "^6.1.4"
  },
  "dependencies": {
    "@aws-sdk/client-cloudwatch-node": "^0.1.0-preview.2",
    "@aws-sdk/client-resource-groups-tagging-api-node": "0.1.0-preview.2",
    "@babel/preset-env": "^7.13.12",
    "@babel/preset-react": "^7.13.13",
    "aws-sdk": "^2.875.0",
    "axios": "^0.21.1",
    "babel-jest": "^26.6.3",
    "base32-encode": "^1.1.1",
    "compression": "^1.7.4",
    "cookie-session": "^2.0.0-rc.1",
    "csurf": "^1.11.0",
    "d3": "^6.6.2",
    "d3-array": "^2.12.1",
    "d3-axis": "^2.1.0",
    "d3-sankey": "^0.12.3",
    "d3-scale": "^3.2.4",
    "d3-selection": "^2.0.0",
    "d3-shape": "^2.1.0",
    "express": "^4.17.1",
    "express-pino-logger": "^6.0.0",
    "express-static-gzip": "^2.1.1",
    "fnv-plus": "^1.3.1",
    "govuk-frontend": "^3.11.0",
    "happy-dom": "^2.11.2",
    "helmet": "^4.4.1",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "nanoid": "^3.1.22",
    "node-jose": "^2.0.0",
    "node-zendesk": "^2.0.5",
    "notifications-node-client": "^5.1.0",
    "openid-client": "^4.6.0",
    "p-limit": "^3.1.0",
    "passport": "^0.4.1",
    "passport-oauth2": "^1.5.0",
    "pino": "^6.11.2",
    "qs": "^6.10.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-markdown": "^5.0.3",
    "route-parser": "^0.0.5",
    "source-map-support": "^0.5.19",
    "tslib": "^2.1.0",
    "uuid": "^8.3.2"
  },
  "engines": {
    "node": "12.x.x",
    "npm": ">=6.x.x"
  },
  "jest": {
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "<rootDir>/config/",
      "<rootDir>/src/components/errors/",
      "index.ts"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ],
    "moduleNameMapper": {
      "^.+\\.(css|scss|sass)$": "<rootDir>/config/jest/file.js"
    },
    "transform": {
      "^.+\\.(js|jsx)$": "babel-jest",
      "^.+\\.(ts|tsx)$": "ts-jest",
      "^.+\\.(css|scss|sass)$": "identity-obj-proxy",
      "^.+\\.(pegjs)$": "<rootDir>/config/jest/pegjs.js",
      "^.+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/config/jest/file.js"
    },
    "globals": {
      "ts-jest": {
        "tsconfig": "tsconfig.json",
        "diagnostics": false
      }
    },
    "testMatch": [
      "<rootDir>/src/**/*.test.+(ts|tsx|js|jsx)"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/config/jest/enzyme.config.js"
    ]
  }
}
